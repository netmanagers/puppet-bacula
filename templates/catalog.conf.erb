Catalog {
  Name = "<%= @name %>"
  DBDriver = <%= @db_driver %>
  DBAddress = <%= @db_address %>
  DBPort = <%= @db_port %>
<% if scope.lookupvar('bacula::@db_name') != '' -%>
<% if scope.lookupvar('bacula::@db_user') != '' -%>
<% if scope.lookupvar('bacula::@db_password') != '' -%>
  DBName = <%= @db_name %>; dbuser = <%= @db_user %>; dbpassword = <%= @db_password %>;
<% end -%>
<% end -%>
<% end -%>
}
